<?xml version="1.0" encoding="UTF-8"?>

<openbox_menu>

<menu id="apps-accessories-menu" label="Accessories">
  <item label="Calculator">
    <action name="Execute">
      <command>gnome-calculator</command>
    </action>
  </item>
  <item label="Character Map">
    <action name="Execute">
      <command>gnome-character-map</command>
    </action>
  </item>
  <item label="Directories">
    <action name="Execute">
      <command>pcmanfm</command>
    </action>
  <item label="Terminal">
    <action name="Execute">
      <command>xterm</command>
    </action>
  </item>
  <item label="Dropbox">
    <action name="Execute">
      <command>dropboxd</command>
    </action>
</menu>

<menu id="apps-office-menu" label="Office">
  <item label="Leafpad">
    <action name="Execute">
      <command>leafpad</command>
    </action>
  </item>
  <item label="Documents">
    <action name="Execute">
      <command>abiword</command>
    </action>
  </item>
  <item label="Spreadsheets">
    <action name="Execute">
      <command>gnumeric</command>
    </action>
  </item>
</menu>

<menu id="apps-graphics-menu" label="Graphics">
  <item label="Editor">
    <action name="Execute">
      <command>gimp</command>
    </action>
  </item>
  <item label="Viewer">
    <action name="Execute">
      <command>viewnior</command>
    </action>
  </item>
  <item label="Diagrams">
    <action name="Execute">
      <command>dia</command>
    </action>
  </item>
  <item label="Vectors">
    <action name="Execute">
      <command>inkscape</command>
    </action>
  </item>
</menu>

<menu id="apps-net-menu" label="Internet">
  <item label="Browser">
    <action name="Execute">
      <command>firefox</command>
    </action>
  </item>
  <item label="E-Mail">
    <action name="Execute">
      <command>thunderbird</command>
    </action>
  </item>
  <item label="FTP">
    <action name="Execute">
      <command>filezilla</command>
    </action>
  </item>
  <item label="Instant Messenger">
    <action name="Execute">
      <command>pidgin</command>
    </action>
  </item>
  <item label="SSH">
    <action name="Execute">
      <command>xterm -e ssh zach@192.168.1.118 -p 2222</command>
    </action>
  </item>
</menu>

<menu id="apps-mpd-menu" label="MPD">
  <item label="MPD Controller">
    <action name="Execute">
      <command>sonata</command>
    </action>
  </item>
  <item label="MPD Listen">
    <action name="Execute">
      <command>mplayer -playlist /home/zach/downloads/mpd.ogg.m3u</command>
    </action>
  </item>
</menu>

<menu id="system-menu" label="System">
  <item label="Openbox Configuration Manager">
    <action name="Execute">
      <command>obconf</command>
    </action>
  </item>
  <item label="GTK Appearance">
    <action name="Execute">
      <command>lxappearance</command>
    </action>
  </item>
  <item label="Reconfigure Openbox">
    <action name="Reconfigure" />
  </item>
  <separator />
  <item label="Reboot">
    <action name="Execute">
      <command>sudo reboot</command>
    </action>
  </item>
  <item label="Power Off">
    <action name="Execute">
      <command>sudo poweroff</command>
    </action>
  </item>
  <item label="Sleep">
    <action name="Execute">
      <command>sudo pm-suspend</command>
    </action>
  </item>
</menu>

<menu id="root-menu" label="Openbox 3">
  <separator label="Applications" />
  <menu id="apps-accessories-menu"/>
  <menu id="apps-office-menu"/>
  <menu id="apps-graphics-menu"/>
  <menu id="apps-net-menu"/>
  <menu id="apps-mpd-menu"/>
  <separator label="System" />
  <menu id="system-menu"/>
  <separator />
  <item label="Log Out">
    <action name="Exit">
      <prompt>yes</prompt>
    </action>
  </item>
</menu>

</openbox_menu>
